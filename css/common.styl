$blue = #027da9
$orange = #ff8400

$sans-serif = "Lato", Arial, sans-serif

p
  line-height: 1.5em

li
  line-height: 1.5em

#_STANDARD_BUTTON
  background: #00305c
  border-radius: 4px
  color: #f0f0f0
  display: inline-block
  font-family: "Lato", sans-serif
  margin: 0 2px
  padding: 14px 30px
  text-align: center
  text-decoration: none

  &.orange
    background: $orange
    color: white
