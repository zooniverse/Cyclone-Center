{
  "name": "cyclone-center",
  "version": "0.1.0",
  "dependencies": {
    "haw": "~0.4.9",
    "navigable": "~0.0.8",
    "publisssh": "~0.2.5",
    "t7e": "~0.3.1",
    "zooniverse": "~0.6.10",
    "zootorial": "~0.3.5"
  },
  "scripts": {
    "start": "haw serve",
    "deploy-to-staging": "haw build && publisssh ./build zooniverse-static/www.cyclonecenter.org/redux && trash ./build",
    "deploy-to-production": "haw build && publisssh --local ./build --bucket zooniverse-static/www.cyclonecenter.org && trash ./build"
  }
}
